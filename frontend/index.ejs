	<div id="App">
		<div class="header-wrapper">
			<header class="navbar inverse text-center">
				<a class="navbar-brand btn btn-link" style="border:none !important">Boilerplate</a>
			</header>
		</div>
		<div class="content-wrapper">
			<div class="loading"></div>
			<p class="text-center">Inhalt wird geladen...</p>
		</div>
	</div>
</body>
</html>